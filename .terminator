[global_config]
  enabled_plugins = ,
  focus = system
  geometry_hinting = True
  inactive_color_offset = 0.8
  suppress_multiple_term_dialog = True
  tab_position = bottom
  title_font = Inconsolata 12
  title_hide_sizetext = True
  title_use_system_font = False
[keybindings]
  close_term = <Primary><Shift><Alt>w
  close_window = None
  next_tab = <Primary><Alt>equal
  prev_tab = <Primary><Alt>minus
  switch_to_tab_2 = None
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      profile = default
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    background_color = "#1e1e1e"
    background_darkness = 0.9
    background_type = transparent
    copy_on_selection = True
    cursor_color = "#f0f0f0"
    custom_command = /bin/zsh
    font = Inconsolata 18
    foreground_color = "#ffffff"
    icon_bell = False
    login_shell = True
    palette = "#333333:#dc322f:#859900:#b58900:#268bd2:#d33682:#2aa198:#eee8d5:#555753:#cb4b16:#586e75:#657b83:#839496:#697678:#93a1a1:#fdf6e3"
    scroll_on_output = True
    scrollback_infinite = True
    scrollback_lines = 5000
    scrollbar_position = hidden
    show_titlebar = False
    use_custom_command = True
    use_system_font = False
